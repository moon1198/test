main	a.c	/^void main(){$/;"	f
main	hello-str.c	/^int main () {$/;"	f
my_itoa	stdio.c	/^void my_itoa (char *buf, int num, int hex){$/;"	f
my_memcpy	string.c	/^void *my_memcpy(void *out, const void *in, int n) {$/;"	f
my_memset	stdio.c	/^void *my_memset(void *s, int c, int n){$/;"	f
my_sprintf	stdio.c	/^int my_sprintf(char *out, const char *fmt, ...) {$/;"	f
my_strcat	string.c	/^char *my_strcat(char *dst, const char *src) {$/;"	f
my_strcmp	string.c	/^int my_strcmp(const char *s1, const char *s2) {$/;"	f
my_vsprintf	stdio.c	/^int my_vsprintf(char *out, const char *fmt, va_list ap) {$/;"	f
